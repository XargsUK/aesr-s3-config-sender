{
  "name": "aesr-s3-config-sender",
  "version": "1.0.0-beta.1",
  "description": "AWS Extend Switch Roles config sender",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production",
    "build:chrome": "npm run build:prod && npm run update-credits && npm run package:chrome",
    "build:firefox": "npm run build:prod && npm run update-credits && npm run package:firefox",
    "package:chrome": "node scripts/package.js chrome",
    "package:firefox": "node scripts/package.js firefox",
    "type-check": "tsc --noEmit",
    "update-credits": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/updateCredits.ts",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "prod": "cross-env NODE_ENV=production webpack --mode production",
    "start:chrome": "npm run build:dev && cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/launchDev.ts chrome",
    "start:firefox": "npm run build:dev && cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/launchDev.ts firefox",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "author": "Bradley Duncan",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.454.0",
    "@aws-sdk/client-sts": "^3.454.0",
    "fast-xml-parser": "^4.3.2",
    "lucide": "^0.294.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/node": "^20.10.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "archiver": "^6.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "glob": "^10.3.10",
    "install": "^0.13.0",
    "license-checker": "^25.0.1",
    "mini-css-extract-plugin": "^2.7.5",
    "npm": "^10.2.4",
    "prettier": "^3.1.0",
    "purgecss-webpack-plugin": "^5.0.0",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  }
}
