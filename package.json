{
  "name": "aesr-s3-config-sender",
  "version": "1.0.0-beta.1",
  "description": "AWS Extend Switch Roles config sender",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production",
    "build:chrome": "npm run build:prod && npm run update-credits && npm run package:chrome",
    "build:firefox": "npm run build:prod && npm run update-credits && npm run package:firefox",
    "package:chrome": "node scripts/package.js chrome",
    "package:firefox": "node scripts/package.js firefox",
    "type-check": "tsc --noEmit",
    "update-credits": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/updateCredits.ts",
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "prod": "cross-env NODE_ENV=production webpack --mode production",
    "start:chrome": "npm run build:dev && cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/launchDev.ts chrome",
    "start:firefox": "npm run build:dev && cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/launchDev.ts firefox",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "Bradley Duncan",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/client-sts": "^3.758.0",
    "fast-xml-parser": "^4.5.3",
    "glob-stream": "^8.0.2",
    "lucide": "^0.462.0",
    "minipass": "^7.1.2",
    "path-scurry": "^2.0.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.287",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "archiver": "^6.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "glob": "^10.4.5",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "license-checker": "^25.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "npm": "^10.9.2",
    "prettier": "^3.5.3",
    "purgecss-webpack-plugin": "^5.0.0",
    "rimraf": "^5.0.10",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
